page {
  background-color: #f6f6f6;
}
.header{
   text-align: center;
  margin-top: 32rpx;
}
.city{
  background-color: white;
  display: inline-block;
  padding:8rpx 18rpx;
  text-align: center;
  border-radius: 10rpx;
  box-shadow: 0.1rem 0.1rem 1rem rgba(0, 0, 0, 0.15);
}
.up-time{
  margin-right: 32rpx;
  text-align: right;
  font-size: 24rpx;
}
.weui-icon{
  margin-top: -8rpx;
}
.oli{
  display: flex;
  padding: 0 32rpx;
   justify-content: space-between;
  align-content: center;
  align-items: center;
  overflow: hidden;
  margin: 32rpx;
  background-color: white;
   border-radius: 18rpx;
  box-shadow: 0.1rem 0.1rem 1rem rgba(0, 0, 0, 0.15);
}
.oli-name{
  display: inline-block;
  font-size: 50rpx;
  float: left;
}
.oli-price{
  color: #FE9A2E;
  display: inline-block;
  font-size: 80rpx;
  font-weight: bold;
  float: right;
}
.oli-price text{
  color: black;
  font-size: 28rpx;
  margin-left: 18rpx;
}
.oiling{
  color: white;
  font-size: 50rpx;
  background-color: #FE9A2E;
  border-radius: 100rpx;
  display: inline-block;
  padding: 60rpx 25.75rpx;
  box-shadow: 0.1rem 0.1rem 1rem rgba(0, 0, 0, 0.15);
}
.footer{
  margin: 64rpx 0;
  text-align: center;
}